<?xml version="1.0" encoding="UTF-8" ?>
<schema name="GroupInfo" version="1.6">
	<!-- ID机构用户ID -->
	<field name="Id" type="string" indexed="true" stored="true" required="true" />
	<!-- 用户类型 -->
	<field name="UserType" type="string" indexed="true" stored="true"/>
	<!-- 用户密码 -->
	<field name="Password" type="string" indexed="true" stored="true"/>
	<!-- 机构名称 -->
	<field name="Institution" type="string" indexed="true" stored="true"/>
	<!-- 登录方式 -->
	<field name="LoginMode" type="string" indexed="true" stored="true"/>
	<!-- 父ID -->
	<field name="ParentId" type="string" indexed="true" stored="true"/>
	<!-- 是否冻结 -->
	<field name="IsFreeze" type="boolean" indexed="true" stored="true"/>
	<!-- 机构用户购买项目 -->
	<field name="PayChannelId" type="string" indexed="true" stored="true" multiValued="true"/>
	<!-- 工单类型 -->
	<field name="OrderType" type="string" indexed="true" stored="true"/>
	<!-- 工单号 -->
	<field name="OrderContent" type="string" indexed="true" stored="true"/>
	<!-- 国别 -->
	<field name="CountryRegion" type="string" indexed="true" stored="true"/>
	<!-- 地区 -->
	<field name="PostCode" type="string" indexed="true" stored="true"/>
	<!-- 机构类型 -->
	<field name="Organization" type="string" indexed="true" stored="true"/>
	<!-- 是否有子账号 -->
	<field name="HasChildGroup" type="boolean" indexed="true" stored="true"/>
	<!-- 统计分析权限 -->
	<field name="StatisticalAnalysis" type="string" indexed="true" stored="true" multiValued="true"/>
	<!-- 此字段判断(试用项目\APP嵌入服务\微信公众号嵌入服务\党建管理员) -->
	<field name="GroupRole" type="string" indexed="true" stored="true"/>
	<!-- 购买项目是否使用 -->
	<field name="IsTrial" type="string" indexed="true" stored="true" multiValued="true"/>
	<!-- IP段 -->
	<field name="OpenIP" type="string" indexed="true" stored="true" multiValued="true"/>
	<!-- 开始IP -->
	<field name="StartIP" type="long" indexed="true" stored="true"/>
	<!-- 结束IP -->
	<field name="EndIP" type="long" indexed="true" stored="true"/>
	<!-- 机构管理员ID -->
	<field name="AdministratorId" type="string" indexed="true" stored="true"/>
	<!-- 机构管理员密码 -->
	<field name="AdministratorPassword" type="string" indexed="true" stored="true"/>
	<!-- 机构管理员邮箱 -->
	<field name="AdministratorEmail" type="string" indexed="true" stored="true"/>
	<!-- 机构管理员IP -->
	<field name="AdministratorOpenIP" type="string" indexed="true" stored="true" multiValued="true"/>
	<!-- 子账号上限 -->
	<field name="ChildGroupLimit" type="int" indexed="true" stored="true"/>
	<!--子账号并发数上限  -->
	<field name="ChildGroupConcurrent" type="int" indexed="true" stored="true"/>
	<!--  机构账号并发数-->
	<field name="GroupConcurrent" type="int" indexed="true" stored="true"/>
	<!-- 子账号下载量（点击次数）上限/天 -->
	<field name="ChildGroupDownloadLimit" type="int" indexed="true" stored="true"/>
	<!-- 设置子账号扣款模式 -->
	<field name="ChildGroupPayment" type="int" indexed="true" stored="true"/>
	<!-- 开通app开始时间 -->
	<field name="AppStartTime" type="string" indexed="true" stored="true"/>
	<!-- 开通app结束时间 -->
	<field name="AppEndTime" type="string" indexed="true" stored="true"/>
	<!-- 开通微信app开始时间 -->
	<field name="WeChatStartTime" type="string" indexed="true" stored="true"/>
	<!-- 开通微信app结束时间 -->
	<field name="WeChatEndTime" type="string" indexed="true" stored="true"/>
	<!-- 开通微信app的邮箱 -->
	<field name="Email4WeChat" type="string" indexed="true" stored="true"/>
	<!-- 党建管理员开始时间 -->
	<field name="PartyAdminStartTIme" type="string" indexed="true" stored="true"/>
	<!-- 党建管理员结束时间 -->
	<field name="PartyAdminEndTIme" type="string" indexed="true" stored="true"/>
	<!-- 党建管理员用户名 -->
	<field name="PartyAdminId" type="string" indexed="true" stored="true"/>
	<!-- 党建管理员密码 -->
	<field name="PartyAdminPassword" type="string" indexed="true" stored="true"/>
